apiVersion: v1
kind: Config
clusters:
  - cluster:
      certificate-authority: /var/state/root.cert
      server: https://leader.telekube.local:6443
    name: kubernetes
contexts:
  - context:
      cluster: kubernetes
      user: agent
    name: agent-to-kubernetes
current-context: agent-to-kubernetes
users:
  - name: coredns
    user:
      client-certificate: /var/state/planet-agent.cert
      client-key: /var/state/planet-agent.key
